<h1>(アトラクション口コミ)</h1>
<h3>新規投稿ページ</h3>
<%= form_for @tweet do |f| %>


  <div class="field">
    <%= f.label :名前 %>
    <%= f.text_field :name, :size => 30 %>
  </div>


  <div class="field">
    <%= f.label :アクセス %>
    <%= f.text_field :access, :access => 30 %>
  </div>


  <div class="field">
    <%= f.label :コメント %>
    <%= f.text_field :comments, :size => 30 %>
  </div>




 <div class="field">
    <%= f.label :画像%>
    <%= f.file_field :image %>
  </div>

  <%= f.submit "投稿する" %>
<% end %>

<%= link_to "一覧に戻る", tweets_path %>